{
  "name": "zfegg/attachment-handler",
  "description": "附件上传组件",
  "license": "MIT",
  "authors": [
    {
      "name": "Moln",
      "email": "moln.xie@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Zfegg\\AttachmentHandler\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ZfeggTest\\AttachmentHandler\\": "test/"
    }
  },
  "require": {
    "php": ">=8.0",
    "ext-json": "*",
    "psr/container": "^1.0 | ^2.0",
    "psr/http-factory": "^1.0",
    "psr/http-server-handler": "^1.0",
    "siriusphp/validation": "^3.0",
    "league/flysystem": "^3",
    "laminas/laminas-diactoros": "^2.8 | ^3.0"
  },
  "extra": {
    "laminas": {
      "config-provider": "Zfegg\\AttachmentHandler\\ConfigProvider"
    },
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  },
  "require-dev": {
    "slevomat/coding-standard": "^8.0",
    "iidestiny/flysystem-oss": "^4",
    "laminas/laminas-servicemanager": "^3.10",
    "phpunit/phpunit": "^9.5",
    "laminas/laminas-config-aggregator": "^1.13",
    "league/flysystem-ftp": "^3.29",
    "league/flysystem-memory": "^3.29",
    "league/flysystem-ziparchive": "^3.29",
    "league/flysystem-sftp-v3": "^3.29"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf"
  }
}
